[project]
	description = Hardening Guide for OpenStack, Created by the OpenStack Security Group
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Robert (hyakuhei)
[access "refs/for/refs/*"]
	read = group user/Robert (hyakuhei)
	push = group user/Robert (hyakuhei)
	label-Code-Review = -2..+2 group user/Robert (hyakuhei)
	label-Verified = -1..+1 group user/Robert (hyakuhei)
	rebase = group user/Robert (hyakuhei)
	removeReviewer = group user/Robert (hyakuhei)
	submit = group user/Robert (hyakuhei)
[access "refs/heads/*"]
	read = group user/Robert (hyakuhei)
	create = group user/Robert (hyakuhei)
	push = +force group user/Robert (hyakuhei)
	pushMerge = group user/Robert (hyakuhei)
[access "refs/tags/*"]
	push = +force group user/Robert (hyakuhei)
	pushTag = group user/Robert (hyakuhei)
	pushSignedTag = group user/Robert (hyakuhei)
